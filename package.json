{
  "name": "huxy-node-server",
  "version": "1.0.8",
  "description": "一个精炼、高性能的 Express.js 服务器模板，为现代 Node.js 应用程序设计，提供灵活的功能和最佳实践。",
  "type": "module",
  "module": "./src/index.js",
  "exports": {
    ".": {
      "import": "./src/index.js"
    },
    "./*": {
      "import": "./src/*.js"
    }
  },
  "scripts": {
    "start": "NODE_ENV=production node ./example.js",
    "dev": "NODE_ENV=development node --watch ./example.js"
  },
  "keywords": [
    "express",
    "nodejs",
    "esm",
    "server",
    "huxy"
  ],
  "author": "ahyiru",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ahyiru/huxy-node-server/issues"
  },
  "homepage": "https://github.com/ahyiru/huxy-node-server#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ahyiru/huxy-node-server.git"
  },
  "dependencies": {
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0",
    "pino": "^10.2.1",
    "pino-http": "^11.0.0",
    "pino-pretty": "^13.1.3"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
